    "import numpy as np\n",
    "\n",
    "def random_colors(N):\n",
    "    colors = [tuple(255 * np.random.rand(3)) for _ in range(N)]\n",
    "    return colors\n",
    "\n",
    "n_instances = 18\n",
    "\n",
    "colors = random_colors(n_instances)\n",
    "\n",
    "for n, color in enumerate(colors):\n",
    "    print(n,color)\n",
    "    \n"
